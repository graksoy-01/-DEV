for x in range(288, 56000):
    a = x * (x + 1)/2
    for z in range(166, 32000):
        b = z * ((3 * z) - 1)/2
        if a==b:
            for y in range(144, 28000):
                c = y * ((2 * y) - 1)
                if a == b and b == c and a == c:
                    print(x)
                    print(z)
                    print(y)
                    print("------")
                    print(a)
                    break
